include includes/header.jade
section.results
    aside
        a.results__back(href = "model.html") Back
        .filter
            h4.filter__title Pages Per Minute
            a.filter__item(href = "results-filtered.html") 20 - 35
            a.filter__item(href = "results-filtered.html") 36 - 45
            a.filter__item(href = "results-filtered.html") 46 - 55
            a.filter__item(href = "results-filtered.html") 56 - 65
            a.filter__item(href = "results-filtered.html") 66 - 75
            a.filter__item(href = "results-filtered.html") +75
    main
        h1 Your head-to-head comparisons are ready!
        - var n = 0
        while n < 4
            article.results__item
                img.results__img(src = 'imgs/img-MX-M354N.jpg')
                h2.results__title Sharp MX-M354N
                span.results__competition Canon imageRUNNER ADVANCE 4035
                a.results__btn(href = "download.html") Download Report
                - n++
include includes/footer-noprinter.jade
